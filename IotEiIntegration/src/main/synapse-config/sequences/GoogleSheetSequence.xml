<?xml version="1.0" encoding="UTF-8"?>
<sequence name="GoogleSheetSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <sequence key="UseInboundPayload"/>
    <property action="remove" name="REST_URL_POSTFIX" scope="axis2"/>
    <call blocking="true">
        <endpoint>
            <http method="post" uri-template="https://sheets.googleapis.com/v4/spreadsheets/190Ym4DG9s7Nk_YxA8cr1D_8VEmNpCnYsskyYgZxdvac/values/Sheet1:append?valueInputOption=RAW">
